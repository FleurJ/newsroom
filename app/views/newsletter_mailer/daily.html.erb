<div class="container">
  <div class="card">
    <div class="card-header">
      <p><%= @newsletter.title %></p>
      <p><%= simple_format(@newsletter.body) %></p>
    </div>
    <br>
    <div class="card-body">
      <% @newsletter.articles.each do |article|  %>
        <p> <%= link_to article.title, article_url(article) %></p>
      <% end %>
    </div>
  </div>


